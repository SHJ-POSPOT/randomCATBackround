{
	"manifest_version": 3,
	"name": "Unsplash Cat Background",
	"version": "1.0",
	"description": "Changes the background of the browser to a random cat photo from Unsplash",
	"icons": {
	  "16": "icons/icon16.png",
	  "32": "icons/icon32.png",
	  "64": "icons/icon64.png",
	  "128": "icons/icon128.png"
	},
	"background": {
	  "service_worker": "background.js"
	},
	"action": {
		"default_icon": {
			"16": "icons/icon16.png"
		}
	  },
	"permissions": [
		"storage",
		"activeTab",
		"scripting"
	],
	"optional_permissions": [
		"topSites"
	  ],
	  "host_permissions": ["<all_urls>"]
  }
  